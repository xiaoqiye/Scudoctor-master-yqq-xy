<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scudoctor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainFragmentActivity"></activity>
        <activity android:name=".view.LoginActivity" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".view.ChangePasswordActivity" />
        <activity android:name=".view.FindPasswordActivity" />
        <activity android:name=".view.RealNameActivity" />
        <activity android:name=".view.PerfectInfoActivity"/>
    </application>

</manifest>